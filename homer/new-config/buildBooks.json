{
  "corpus":true,
  "indexPath" : "portland-3.5/books",
  "galagoJobDir" : "job/books",
  "inputPath" : ["new-config/portland.list"],
  "tokenizer" : {
    "fields" : [ "name", "title", "creator", "language", "subject", "date" ] , 
