- import ciir.proteus.ProteusObject
-# The import below loads the local namespace of pObject's
-# instance members.
-@ import val pObject : ProteusObject
.container-fluid
 .row-fluid
  %h1 #{id.`type`}: #{title.getOrElse("<Unknown>")}
  %hr
  -# The remainder the information is rendered specific to the
     type held.
  = view(pObject, id.`type`.name)